{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "format", "lint", "prelint", "test"]
      }
    }
  },
  "targetDefaults": {
    "lint": {
      "dependsOn": ["prelint", "^prelint", "format", "^format"]
    },
    "build": {
      "dependsOn": ["lint", "^lint"]
    },
    "test": {
      "dependsOn": ["build", "^build"]
    }
  }
}
