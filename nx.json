{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default"
    }
  },
  "targetDefaults": {
    "lint": {
      "dependsOn": [
        "prelint",
        "^prelint",
        "format",
        "^format"
      ]
    },
    "test": {
      "dependsOn": [
        "pretest",
        "^pretest",
        "lint",
        "^lint"
      ]
    },
    "build": {
      "dependsOn": [
        "test",
        "^test"
      ]
    }
  }
}
